        &lt;table dir="ltr"&gt;
            &lt;tr&gt;
                &lt;td id="i1" style="padding:0;font-family:'Segoe UI Semibold', 'Segoe UI Bold', 'Segoe UI', 'Helvetica Neue Medium', Arial, sans-serif;font-size:17px;color:#707070;"&gt;{0} konta&lt;/td&gt;
            &lt;/tr&gt; 
            &lt;tr&gt;
