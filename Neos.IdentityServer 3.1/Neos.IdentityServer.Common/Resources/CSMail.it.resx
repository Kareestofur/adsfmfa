        &lt;table dir="ltr"&gt;
	    &lt;tr&gt;
                &lt;td id="i0" style="padding:0;padding-top:25px;font-family:'Segoe UI', Tahoma, Verdana, Arial, sans-serif;font-size:14px;color:#2a2a2a;"&gt;&lt;span style="font-family:'Segoe UI Bold', 'Segoe UI Semibold', 'Segoe UI', 'Helvetica Neue Medium', Arial, sans-serif;font-size:18px;font-weight:bold;color:#2a2a2a;"&gt; &lt;img id="logo" src="cid:{5}" /&gt;&lt;/span&gt; &lt;/td&gt;
